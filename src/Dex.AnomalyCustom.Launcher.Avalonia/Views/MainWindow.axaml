<Window
    x:Class="Dex.AnomalyCustom.Launcher.Avalonia.Views.MainWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:v="using:Dex.AnomalyCustom.Launcher.Avalonia.Views"
    Title="Anomaly Custom Launcher"
    Width="1000"
    Height="600"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Icon="/Assets/Logo.ico"
    mc:Ignorable="d">
    <Window.Background>
        <ImageBrush
            Source="../Assets/Background/background_1.png"
            Stretch="UniformToFill" />
    </Window.Background>

    <v:MainView />
</Window>
